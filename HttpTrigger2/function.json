{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
          "get"
      ],
      "route": "staff/{bldg}"
  },
  {
      "type": "http",
      "direction": "out",
      "name": "res"
  },
  {
      "name": "todoItems",
      "type": "sql",
      "direction": "in",
      "commandText": "select * from dbo.tempEmployeeData where AccBuildingCode = @Bldg",
      "commandType": "Text",
      "parameters": "@Bldg={bldg}",
      "connectionStringSetting": "SqlConnectionString"
  }
  ]
}